        <img id="image"
         src="file:///F:/Ostad/1.webp"
         height=""
         width="" 
        ><br><br>
        <button class="btn btn-success" onclick="ImageOne()">Switch to Image 1</button> <button class="btn btn-success" onclick="ImageTwo()">Switch to Image 2</button>
        <script>
            ImageOne = () =>{
                let image = document.getElementById("image");
                image.src = "file:///F:/Ostad/1.webp"
            }
            ImageTwo = () =>{
                let image = document.getElementById("image");
                image.src = "file:///F:/Ostad/2.webp"
            }
        </script>
